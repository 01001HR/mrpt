									<border>5</border>
									<option>1</option>
								</object>
							</object>
						</object>
						<label>2) Capture</label>
					</object>
					<object class="notebookpage">
						<object class="wxPanel" name="ID_PANEL5" variable="Panel5" member="yes" />
						<label>3) Verify</label>
					</object>
					<object class="notebookpage">
						<object class="wxPanel" name="ID_PANEL6" variable="Panel6" member="yes" />
