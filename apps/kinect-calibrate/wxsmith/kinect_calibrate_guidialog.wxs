																				<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
																				<border>5</border>
																				<option>1</option>
																			</object>
																			<object class="sizeritem">
																				<object class="wxSpinCtrl" name="ID_SPINCTRL2" variable="edSizeY" member="yes">
																					<value>8</value>
