																<border>5</border>
																<option>1</option>
															</object>
															<object class="sizeritem">
																<object class="wxStaticBoxSizer" variable="StaticBoxSizer5" member="no">
																	<label> Size of quads (in mm): </label>
																	<object class="sizeritem">
																		<object class="wxFlexGridSizer" variable="FlexGridSizer18" member="no">
																			<cols>4</cols>
																			<rows>1</rows>
																			<object class="sizeritem">
																				<object class="wxStaticText" name="ID_STATICTEXT3" variable="StaticText3" member="yes">
																					<label>In X:</label>
																				</object>
																				<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
																				<border>5</border>
																				<option>1</option>
																			</object>
																			<object class="sizeritem">
																				<object class="wxTextCtrl" name="ID_TEXTCTRL1" variable="edLengthX" member="yes">
																					<value>40.0</value>
																					<size>40,-1</size>
																				</object>
																				<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
																				<border>5</border>
																				<option>1</option>
																			</object>
																			<object class="sizeritem">
																				<object class="wxStaticText" name="ID_STATICTEXT4" variable="StaticText4" member="yes">
																					<label>In Y:</label>
																				</object>
																				<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
																				<border>5</border>
																				<option>1</option>
																			</object>
																			<object class="sizeritem">
																				<object class="wxTextCtrl" name="ID_TEXTCTRL3" variable="edLengthY" member="yes">
																					<value>40.0</value>
																					<size>40,-1</size>
																				</object>
																				<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
																				<border>5</border>
																				<option>1</option>
																			</object>
																		</object>
																		<flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
																		<option>1</option>
																	</object>
																</object>
																<flag>wxALL|wxEXPAND|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
																<border>5</border>
																<option>1</option>
